{
  "name": "mapper-factory",
  "version": "4.0.0",
  "description": "mapper for typescript object",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "/dist",
    "/images"
  ],
  "scripts": {
    "build": "npx tsc",
    "dev": "npx tsc --watch",
    "test": "npx tsx src/test.ts",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major",
    "deploy": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucaAngrisani/mapper-factory.git"
  },
  "keywords": [
    "mapper-ts",
    "mapper-factory",
    "mapper",
    "factory",
    "mapper factory"
  ],
  "author": "lucaAngrisani Angrigo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lucaAngrisani/mapper-factory/issues"
  },
  "homepage": "https://github.com/lucaAngrisani/mapper-factory#readme",
  "dependencies": {
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}
